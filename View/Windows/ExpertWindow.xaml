<Window x:Class="CourseWork_SAIPIS.ExpertWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CourseWork_SAIPIS"
        mc:Ignorable="d"
        Title="ExpertWindow" Height="450" Width="800">
    <Grid>
        <DataGrid HorizontalAlignment="Left" Height="399" Margin="10,10,0,0" VerticalAlignment="Top" Width="772">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Name}" Header="Мероприятие" Width="385"/>
                <DataGridTextColumn Binding="{Binding Points}" Header="Оценка" Width="385" />
                
            </DataGrid.Columns>
        </DataGrid>
            
    </Grid>
</Window>
